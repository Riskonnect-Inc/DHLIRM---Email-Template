<!-- ViewNotesQuickAction -->
<aura:component controller="ViewNotesQuickAction_Controller" implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes,force:lightningQuickActionWithoutHeader" access="global">
	<aura:attribute name="recordId" type="Id"/>
	<aura:attribute name="noteCatValue" type="Object"/>
    <aura:handler name="init" value="{!this}" action="{!c.init}" />

    <lightning:overlayLibrary aura:id="overlays" />
    <div class="slds-is-relative slds-text-align_center" style="height: 120px">
        <lightning:spinner aura:id="spinner" variant="brand"/>
    </div>
</aura:component>